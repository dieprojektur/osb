<% @open_spaces.each do |open_space| %>
  <tr>
    <td><%= open_space.title %></td>
    <td><%= open_space.location %></td>
    <td><%= open_space.start %></td>
    <td><%= open_space.end %></td>
    <td><%= link_to 'Show', open_space %></td>
    <td><%= link_to 'Edit', edit_open_space_path(open_space) %></td>
    <td><%= link_to 'Destroy', open_space, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

